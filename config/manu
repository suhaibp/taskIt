tbl_project_task
_________________________________________________________________
CREATE TABLE public.tbl_project_task
(
    id serial NOT NULL,
    module_id:integer,
    task_name:string,
    assigned_to_id:integer,
    complexity_id:integer,
    planned_hour double precision,
    description:text,
    start_date_time:date,
    end_date_time:date,
    priority:string,
    task_type:string,
    status_id:integer,
    progress_id:integer,
    actual_start_datetime time(10) with time zone,
    actual_end_datetime time(10) with time zone,
    actual_hour double precision,
    PRIMARY KEY (id)
)
*********************DATA****************************************
INSERT INTO public.tbl_project_task(
module_id, task_name, assigned_to_id, complexity_id, planned_hour, description, start_date_time, end_date_time, priority, task_type, status_id, progress_id, actual_start_datetime, actual_end_datetime, actual_hour)
