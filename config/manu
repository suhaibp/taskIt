tbl_project
_________________________________________________________________
  CREATE TABLE public.tbl_project
  (
      id serial NOT NULL,

      project_name:string, type:string,project_code:string,status:string,priority:string,description:text,is_estimation_completed:boolean,is_estimation_admin_viewed:boolean,estimation_id:integer,planned_start_date:date,planned_end_date:date,actual_start_date:date,actual_end_date:date,
      PRIMARY KEY (id)
  );

  *********************DATA****************************************
